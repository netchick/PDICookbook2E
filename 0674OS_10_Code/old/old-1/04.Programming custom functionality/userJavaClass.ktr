        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[private RowSet shortSentences = null; 
private RowSet longSentences = null; 

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first) //execute only for the first row
    {
        first = false;
        shortSentences = findTargetRowSet("shortOnes");
        longSentences = findTargetRowSet("longOnes"); 
    }

	  r = createOutputRow(r, data.outputRowMeta.size()); 

	  String linein;      
	  linein = get(Fields.In, "line").getString(r);


	long len = linein.length(); 
    //logBasic("LEN="+rev.length());
	
	long qty = 0;
    boolean currentSpecialChar = false; // flag to skip a sequence of separators

    // Parse the sentence
	for (int i=0;i<len;i++) {
		char ch = linein.charAt(i);
		switch(ch) {
			case ',':
			case '.':
			case ' ':
			case ';':
			case ':':
				if (!currentSpecialChar) qty++; //increment the quantity of words
				currentSpecialChar = true;
				break;
			default:
				currentSpecialChar = false;
				break;
		}
	}

	if (!currentSpecialChar) qty++;   //increment the quantity of words with the last word
	get(Fields.Out, "qty").setValue(r, qty);
	
	// Send the row on to the next step.  	
	if (qty < 7){         
		putRowTo(data.outputRowMeta, r, shortSentences);     
		}    
	 else {        
	 putRowTo(data.outputRowMeta, r, longSentences);     
	}   

	return true;
}
]]></class_source>
